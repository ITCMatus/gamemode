public OnPlayerPickUpPickup(playerid, pickupid){
	for(new inter;inter<InteriorCount+1;inter++)
	{
	    if(pickupid == InteriorInfo[inter][Int_Pick])
		{
		    new ext1 = InteriorInfo[inter][Exit_ID];
			SetPlayerPosEx(playerid,ExitInfo[ext1][Int_X],ExitInfo[ext1][Int_Y],ExitInfo[ext1][Int_Z],ExitInfo[ext1][Int_ID],VW_INTERIOR+inter,false);
            SetPlayerFacingAngle(playerid,float(ExitInfo[ext1][Int_ZRot]));
            SetCameraBehindPlayer(playerid);
		}
	}
	for(new ext;ext<ExitCount+1;ext++)
	{
	    if(pickupid == ExitInfo[ext][Exit_Pick])
        {
		    new inte = GetPlayerVirtualWorld(playerid)-VW_INTERIOR;
			SetPlayerPosEx(playerid,InteriorInfo[inte][Exit_X],InteriorInfo[inte][Exit_Y],InteriorInfo[inte][Exit_Z],0,0,false);
			SetPlayerFacingAngle(playerid,float(InteriorInfo[inte][Exit_ZRot]));
			SetCameraBehindPlayer(playerid);
	    	return true;
		}
	}
	#if defined Interior_OnPlayerPickUpPickup
	Interior_OnPlayerPickUpPickup(playerid, pickupid);
    #endif
    return true;
}
#if defined _ALS_OnPlayerPickUpPickup
    #undef OnPlayerPickUpPickup
#else
    #define _ALS_OnPlayerPickUpPickup
#endif
#define OnPlayerPickUpPickup Interior_OnPlayerPickUpPickup
#if defined Interior_OnPlayerPickUpPickup
    forward Interior_OnPlayerPickUpPickup(playerid, pickupid);
#endif

