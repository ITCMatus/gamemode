stock AddInterior(SID,Float:IntX,Float:IntY,Float:IntZ,ExitZRot,ExitID,Local)
{
	if(InteriorCount+1 < MAX_INTERIORS)
	{
		InteriorCount ++;
		InteriorInfo[InteriorCount][Int_SID]      = SID;
		InteriorInfo[InteriorCount][Exit_ID]      = ExitID;
		InteriorInfo[InteriorCount][Exit_X]       = IntX-floatsin(ExitZRot,degrees)*2.5;
		InteriorInfo[InteriorCount][Exit_Y]       = IntY+floatcos(ExitZRot,degrees)*2.5;
		InteriorInfo[InteriorCount][Exit_Z]       = IntZ-0.2;
		InteriorInfo[InteriorCount][Exit_ZRot]    = ExitZRot;
		InteriorInfo[InteriorCount][Int_Lokalita] = Local;
		InteriorInfo[InteriorCount][Int_Pick]     = CreatePickup(19902,1,IntX,IntY,IntZ-0.5);
	}
	else print("Server:: it exceeded the maximum limit Interior");
	return true;
}
stock AddExit(Float:ExitX,Float:ExitY,Float:ExitZ,IntZRot,InteriorID,MapIconMarker)
{
	if(ExitCount+1 < MAX_EXITS)
	{
    	ExitCount ++;
		ExitInfo[ExitCount][Int_ID]    = InteriorID;
		ExitInfo[ExitCount][Int_X]     = ExitX-floatsin(IntZRot,degrees)*2.5;
		ExitInfo[ExitCount][Int_Y]     = ExitY+floatcos(IntZRot,degrees)*2.5;
		ExitInfo[ExitCount][Int_Z]     = ExitZ-0.2;
		ExitInfo[ExitCount][Int_ZRot]  = IntZRot;
		ExitInfo[ExitCount][MapIcon]   = MapIconMarker;
		ExitInfo[ExitCount][Exit_Pick] = CreatePickup(19902,1,ExitX,ExitY,ExitZ-0.5,-1);
   	}
	else print("Server:: it exceeded the maximum limit Interior");
	return true;
}
stock SetPlayerPosEx(playerid,Float:x,Float:y,Float:z,interior,virtualworld,bool:FindZ)
{	
	if(FindZ) SetPlayerPosFindZ(playerid,x,y,z);	
	else      SetPlayerPos     (playerid,x,y,z);
	SetPlayerVirtualWorld(playerid,virtualworld);
	SetPlayerInterior(playerid,interior);
	return true;
}

